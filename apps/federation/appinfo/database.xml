<?xml version="1.0" encoding="ISO-8859-1" ?>
<database>
	<name>*dbname*</name>
	<create>true</create>
	<overwrite>false</overwrite>
	<charset>utf8</charset>
	<table>
		<name>*dbprefix*trusted_servers</name>
		<declaration>
			<field>
				<name>url</name>
				<type>text</type>
				<notnull>true</notnull>
				<primary>true</primary>
				<length>512</length>
				<comments>Url of trusted server</comments>
			</field>
		</declaration>
	</table>
</database>
